{
  "name": "Grid_Based_Inventory_System",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$path": "src/shared"
    },

    "ServerStorage": {
      "$path": "src/stored"
    },

    "ServerScriptService": {
      "InventoryHandlerServer":{
          "$path": "src/server"
        }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "InventoryHandlerClient":{
          "$path": "src/client"
        }
      }
    },

    "Workspace": {
      "$properties": {
        "StreamingEnabled": true,
        "FilteringEnabled": true
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [
            0.38823,
            0.37254,
            0.38823
          ],
          "Locked": true,
          "Position": [
            0,
            -10,
            0
          ],
          "Size": [
            512,
            20,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}